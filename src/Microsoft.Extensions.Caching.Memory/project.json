{
  "version": "1.2.0-*",
  "description": "In-memory cache implementation of Microsoft.Extensions.Caching.Memory.IMemoryCache.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "https://github.com/aspnet/caching"
    },
    "tags": [
      "cache",
      "memorycache"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.Extensions.Caching.Abstractions": {
      "target": "project"
    },
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.2.0-*",
    "Microsoft.Extensions.Options": "1.2.0-*",
    "NETStandard.Library": "1.6.2-*"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        }
      }
    },
    "netstandard1.3": {}
  }
}